<% provide(:title, "New Tenant") %>
<div class="col-md-6 col-md-offset-3">
	<%= form_with(model: @tenant, local: true) do |f| %>

	  <%= f.label :tenant_name %>
	  <%= f.text_field :tenant_name, class: 'form-control' %>

	  <%= f.label :desc %>
	  <%= f.text_field :desc, class: 'form-control' %>

	  <%= f.label :task_start %>
	  <%= f.time_field :task_start, class: 'form-control' %>

	  <%= f.label :task_end %>
	  <%= f.time_field :task_end, class: 'form-control' %>

	  <br>

	  <%= f.submit 'Submit', class: "btn btn-primary" %>

	<% end %>
</div>